<h1>Listing bowling_associations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bowling_associations.each do |bowling_association| %>
    <tr>
      <td><%= bowling_association.name %></td>
      <td><%= link_to 'Show', bowling_association %></td>
      <td><%= link_to 'Edit', edit_bowling_association_path(bowling_association) %></td>
      <td><%= link_to 'Destroy', bowling_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

